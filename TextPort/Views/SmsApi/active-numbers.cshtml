@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Show Active Numbers | Returns a List of Your Active Virtual Numbers | SMS API";
    ViewData["Description"] = "Generate a list of virtual numbers assigned to the SMS API";
    ViewData["Keywords"] = "virtual phone numbers, SMS API numbers, temporary SMS numbers";
    ViewData["Url"] = "https://textport.com/api";
}

