<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"]</title>
    <meta charset="utf-8">
    <meta name="description" content="@ViewData["Description"]">
    <meta property="og:title" content="@ViewData["Title"]">
    <meta property="og:description" content="@ViewData["Description"]">
    <meta property="og:locale" content="en_US">
    <meta property="og:type" content="article">
    <meta property="og:url" content="@ViewData["Url"]">
    <meta property="og:site_name" content="TextPort">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="keywords" content="online texting service, virtual mobile number, bulk texting, group texting, business sms service" />
    <meta name="robots" content="index, follow" />
    <meta name="author" content="TextPort.com" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
